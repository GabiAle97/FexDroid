wget -q http://ports.ubuntu.com/pool/main/g/gst-plugins-base1.0/gir1.2-gst-plugins-base-1.0_1.22.1-1ubuntu1_arm64.deb
wget -q http://ports.ubuntu.com/pool/main/libd/libdrm/libdrm-dev_2.4.114-1_arm64.deb
wget -q http://ports.ubuntu.com/pool/main/libg/libglvnd/libegl-dev_1.6.0-1_arm64.deb
