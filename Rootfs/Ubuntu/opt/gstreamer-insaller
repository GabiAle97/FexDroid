#!/bin/bash
mkdir temp-debs
cd temp-debs
echo "Installing libgstreamer1.0-dev"
mkdir libgstreamer1.0-dev
cd libgstreamer1.0-dev
mkdir depend
wget -q http://ports.ubuntu.com/pool/main/g/gstreamer1.0/libgstreamer1.0-dev_1.22.2-1_armhf.deb
wget -q http://ports.ubuntu.com/pool/main/g/gstreamer1.0/libgstreamer1.0-dev_1.22.2-1_arm64.deb
