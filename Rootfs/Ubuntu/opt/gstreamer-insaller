wget -q http://ports.ubuntu.com/pool/main/g/gst-plugins-base1.0/libgstreamer-gl1.0-0_1.22.1-1ubuntu1.1_armhf.deb
wget -q http://ports.ubuntu.com/pool/main/g/gst-plugins-base1.0/libgstreamer-plugins-base1.0-0_1.22.1-1ubuntu1_armhf.deb
